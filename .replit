modules = ["python-3.11", "nodejs-20"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Character AI Server"

[[workflows.workflow]]
name = "Character AI Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m uvicorn app.main:app --host 0.0.0.0 --port 5000"
